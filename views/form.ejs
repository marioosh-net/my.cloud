<%
var odrabiam = false;
if(typeof form.odrabiam != 'undefined') {
	if(form.odrabiam == '1') {
		odrabiam = true;
	}
} 
%>
<form role="form" class="form-inline" method="post" action="/">
	<div class="form-group">
		<label class="sr-only" for="url">Ścieżka</label>
		<input class="form-control" type="text" style="width: 400px;" id="url" name="url" value="<%if(typeof form.url != 'undefined'){%><%=form.url%><%}%>" />
	</div>			
	<button type="submit" class="btn btn-default">Submit</button>
	<% if(typeof locals.message != 'undefined') { %>
	<div style="width: 400px;" class="validation-message-container">
	<span class="validation-message" style="color: #a00;"><%=message%></span>
	</div>
	<% } %>
	<div class="clear"></div>
</form>
<script>$(function(){
	$('#url').focus();
});</script>