<%
var odrabiam = false;
if(typeof form.odrabiam != 'undefined') {
	if(form.odrabiam == '1') {
		odrabiam = true;
	}
} 
%>
<form id="url-form" role="form" class="form-inline" method="post" action="/">
	<div class="form-group">
		<label class="sr-only" for="url">Ścieżka</label>
		<input class="form-control" type="text" style="width: 600px;" id="url" name="url" value="<%if(typeof form.url != 'undefined'){%><%=form.url%><%}%>" />
	</div>			
	<button id="save" type="button" class="btn btn-default">Zapisz</button>
	<% if(typeof locals.message != 'undefined') { %>
	<div style="width: 400px;" class="validation-message-container">
	<span class="validation-message" style="color: #a00;"><%=message%></span>
	</div>
	<% } %>
	<div class="clear"></div>
</form>

<div id="progress" class="progress" style="margin-top: 10px; display:none;">
  <div id="progress-el" class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 60%;">
  </div>
</div>

<script>$(function(){
	$('#url').focus();
});</script>